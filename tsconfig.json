{
  "extends": "@tsconfig/docusaurus/tsconfig.json",
  "include": [
    "docusaurus.config.js",
    "src/",
    "@docusaurus/module-type-aliases"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "plugins": [
      {
        "name": "typescript-styled-plugin",
        "lint": {
          "validProperties": ["label"]
        }
      }
    ]
  }
}
