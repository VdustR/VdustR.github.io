{
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "pnpm-lock.yaml"
  ],
  "cSpell.words": [
    "browserslist",
    "codegen",
    "fastify",
    "mdsvex",
    "nodejieba",
    "pnpm",
    "podman",
    "shiki",
    "shikijs",
    "sveltejs",
    "vdustr",
    "venv",
    "vite",
    "vitejs"
  ],
  "editor.autoIndent": "full",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true
}
